{
  "version": 2,
  "buildCommand": "cd frontend && npm ci --legacy-peer-deps && npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "cd frontend && npm ci --legacy-peer-deps",
  "framework": "nextjs",
  "functions": {
    "frontend/src/app/api/**/*.ts": {
      "runtime": "nodejs22.x"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}


